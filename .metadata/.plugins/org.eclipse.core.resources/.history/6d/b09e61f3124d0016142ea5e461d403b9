#include "Person.h"
#include <sstream>
#include <iostream>

using namespace std;

Person::Person() {
	// TODO Auto-generated constructor stub
	age = 0;
	name = "";

}

string Person::toString(){
	stringstream ss;

	ss<<"Name: ";
	ss<<name;
	ss<<"; age: ";
	ss<<age;

	return ss.str();
}
