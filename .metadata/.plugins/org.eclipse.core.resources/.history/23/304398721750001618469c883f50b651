#include <iostream>
using namespace std;

int main() {

	string texts[]
				 ={"one", "two", "three"};

	cout<<sizeof(texts)/sizeof(string)<<endl;

	for (int i = 0; i<sizeof(texts)/sizeof(string); i++){
		cout<<	texts[i]<<endl;
	}

	return 0;
}
