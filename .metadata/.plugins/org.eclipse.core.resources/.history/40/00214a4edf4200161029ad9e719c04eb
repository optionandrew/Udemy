#include <iostream>
using namespace std;

int main() {

	bool bValue = true;
	cout<<bValue<<endl;

	char cValue = 7;
	cout << cValue << endl;

	char cValue1 = '7';
	cout << cValue1 << endl;

	cout << int(cValue1) << endl;

	cout<<"size of char: "<<sizeof(char)<<endl;

	return 0;
}
