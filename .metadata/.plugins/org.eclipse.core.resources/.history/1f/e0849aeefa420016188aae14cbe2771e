#include <iostream>
using namespace std;

int main() {

	const string password = "hello";

	cout<<"Enter password: "<<endl;
	string input;
	cin>>input;

	do{
		cout<<"Password incorrect, try again."<<endl;
	} while (password !=input);


	return 0;
}
