#include <iostream>
using namespace std;

int main() {

	int table[9][9] = { 0 };

	for (int i = 1; i <= 9; i++) {
		for (int j = 1; j <= 9; j++) {
			table[i][j] = (i+1) * (j+1);

			cout << table[i][j] << " " << flush;
		}
		cout << endl;
	}

	return 0;
}
