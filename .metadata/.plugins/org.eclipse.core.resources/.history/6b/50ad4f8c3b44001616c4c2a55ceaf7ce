#include <iostream>
using namespace std;

int main() {

              int table[10][10]={0};

              for (int i = 1; i<=8; i++){
                             for (int j = 1; j<=10; j++){
                                           table[i][j]= i*j;
                                           cout<<table[i][j]<<" "<<flush;
                             }
                             cout<<endl;
              }



              return 0;
}
