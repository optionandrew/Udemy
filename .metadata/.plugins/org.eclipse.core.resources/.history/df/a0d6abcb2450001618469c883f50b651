#include <iostream>
using namespace std;

int main() {


	const int NSTRINGS =5;
	string texts[NSTRINGS]={"one","two","three","four","five"};

	string *pTexts = &texts[0];

	//cout<< *pTexts<<endl;

	string *pEnd = &texts[NSTRINGS-1];

	while(pTexts <= pEnd){
		cout<<*pTexts<<endl;
		pTexts++;

	}

	//Set pTexts back to start.
	pTexts=&texts[0];

	int elements = pEnd-pTexts;

	cout<<elements<<flush;






	return 0;
}
